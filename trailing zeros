#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main(){ 
    int test,fact,count=0;
    scanf("%d",&test);
      for(int i=0;i<test;i++){
        scanf("%d",&fact);
          count=0;
          for(;fact>0;fact=fact/5){
              count+=fact/5;
          }
          printf("%d\n",count);
      }
      
    return 0;
}
